/* pages/viewOtherCard/viewOtherCard.wxss */
.page{
    box-sizing: border-box;
    padding: 30rpx;
}
/* pages/completedcard/completedcard.wxss */
.changebox{

    width: 750rpx;
    position: fixed;
    background-color: white;
}
.changeidentity{
    font-size: 36rpx;
    margin-left: 30rpx;

}
.completedcardbody{
    display: flex;
    align-items: center;
    margin-top: 30rpx;
    flex-direction: column;
    letter-spacing: 3rpx;
}
.cardimg{
    width: 690rpx;
    height: 345rpx;
    background-color: rgb(72, 72, 112);
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}
.cardinfo{
    height: 124rpx;
    width: 690rpx;
    box-shadow:
            0px 0px 0px 0px #ffffff,
            -5rpx 0px 30rpx -10px #e6e7e6,
            5rpx 0px 30rpx -10px #e2e4e5,
            0px 10rpx 20rpx 0px #e6e3e3;
    padding-bottom: 30rpx;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;

}
.cardkanguo{
    color: rgba(153, 153, 153, 1);

}
.companyintroduction{
    width: 690rpx;

    margin-top: 20rpx;
    border-radius: 20rpx;
    box-shadow:
            0px 0px 0px 0px #ffffff,
            -5rpx 0px 20rpx 0px #e6e7e6,
            5rpx 0px 20rpx 0px #e2e4e5,
            0px 10rpx 20rpx 0px #e6e3e3;
    margin-bottom: 30rpx;
}
.navbar{
    position: relative;
    height: 88rpx;
    display: flex;
    flex-wrap: nowrap;
    color: rgb(0, 3, 5);
    font-size: 36rpx;
    flex-direction: row;
    align-items: center;

}
.companyintroitem{
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    min-height: 130rpx;
    align-items: center;
}
.introtitle{
    color: rgba(153, 153, 153, 1);
    font-size: 28rpx;
    margin-left: 30rpx;
    width: 150rpx;
}
.introdetail{
    color: rgba(102, 102, 102, 1);
    font-size: 28rpx;
    margin-left: 18rpx;
    width: 350rpx;
}
.introicon{
    color: rgba(10, 163, 239, 1);
    font-size: 26rpx;
    margin-top: -15rpx;
}
.introicontext{
    color: rgba(10, 163, 239, 1);
    font-size: 26rpx;
}
.introbottomborder{
    width: 630rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
    height: 1rpx;
    background-color:rgba(238, 238, 238, 1);
}
.companyintroitem1{

    display: flex;
    flex-direction: row;
    margin-top: 37rpx;
    margin-bottom: 31rpx;
}
.introdetail1{
    color: rgba(102, 102, 102, 1);
    font-size: 28rpx;
    margin-left: 18rpx;
    width: 486rpx;
    white-space: normal;
}
.introductioncover{
    position: absolute;
    width: 690rpx;
    height: 366rpx;
    background:linear-gradient(rgba(0, 0, 0, 0),rgb(250, 247, 247));
    top: 670rpx;
}
.introtitle1{
    color: rgba(153, 153, 153, 1);
    font-size: 28rpx;
    margin-left: 30rpx;
    width: 150rpx;

}
.completedcardbtn_update{
    height: 76rpx;
    width: 300rpx;
    border: rgba(221, 221, 221, 1) solid 1px;
    border-radius: 45rpx;
    display: flex;
    align-items: center;
    font-size: 30rpx;
    justify-content: center;
    color:rgb(102, 102, 102);font-size:28rpx;
    margin-right: 30rpx;
}
.completedcardbtn_update2{
    height: 76rpx;
    width: 300rpx;
    border: rgba(221, 221, 221, 1) solid 1px;
    border-radius: 45rpx;
    font-size: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color:rgba(102, 102, 102);font-size:28rpx;
    margin-right: 30rpx;
}

.completedcardbtn_fenxiang{
    height: 76rpx;
    width: 300rpx;
    border: rgba(221, 221, 221, 1) solid 1px;
    border-radius: 45rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color:rgb(255, 254, 254);font-size:28rpx;
    background-color: rgba(10, 163, 239, 1);
}
.sharebox{
    position: fixed;
    right: 0;
    bottom: 0;
    width: 0;
    height: 450rpx;
    background: #fff;
    border-radius: 20rpx 20rpx;
    z-index: 99;
    opacity: 0;
}
.sharetitle{
    position: relative;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #eee;
}
.sharetitle>view:first-child{
    font-size: 34rpx;
    text-align: center;
}
.closeshare{
    position: absolute;
    top: 30rpx;
    right: 20rpx;
    color: #999;
    transform: scale(1.5,1.2);
    padding: 0 20rpx;
}
.sharebody{
    margin: 20rpx 100rpx;
    display: flex;
    justify-content: space-around;
}
.sharebody>view{
    text-align: center;
}
.sharebody>button{
    text-align: center;
    background: #fff;
    margin: 0;
    padding: 0;
}
.sharebody>button::after{
    border: 0;
}
.sharebody>button>view{
    height: 180rpx;
    font-size: 24rpx;
}
.swiper-box{
    width: 690rpx;
}
.swiper-item{
    width: 690rpx;
    border-radius: 20rpx 20rpx 0 0;
}
.mycard-popup{
    height: 30vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 30rpx;
    margin-top: 20rpx;
}
.share-btn{
    background-color: rgb(255, 255, 255);
}
.share-btn::after{
    border: none;
}
.popbody{
    display: flex;
    margin-top: 70rpx;
}
.popicon{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.righticon{
    margin-right: 200rpx;
}
.popbtn{
    height: 93rpx;
    width: 93rpx;
    border-radius: 46.5rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 26rpx;
    margin-bottom: 27rpx;
}
.oricon{
    background-color: RGB(252, 149, 19);
}
.poptxt{
    font-size: 26rpx;
    color: rgba(51, 51, 51, 1);
}
.icon-a-weixin2{
    color: RGB(40, 196, 69);
    font-size: 93rpx;
}
.icon-haibao{
    color:white;
    font-size: 55rpx;
}
.iconbox{
    width: 40rpx;
    height: 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.default1{
    color: rgba(102, 102, 102);
    
}
.default2{
    color: rgb(253, 253, 8, 0.678);
    
    
}
.collect1{
    
    animation-name: collect1;
    animation-duration:0.3s;
    animation-fill-mode:forwards;
}
@keyframes collect1{
    from{}
    100%{font-size: 15rpx;color: rgba(102, 102, 102);}
    
}
.collect2{
   
    animation-name: collect2;
    animation-duration:0.3s;
    animation-fill-mode:forwards;
}
@keyframes collect2{
    from{font-size: 15rpx;}
    100%{font-size: 30rpx;color: rgb(253, 253, 8, 0.678);}
    
}
.cancel1{
    
    animation-name: cancel1;
    animation-duration:0.3s;
    animation-fill-mode:forwards;
}
@keyframes cancel1{
    from{color: rgb(253, 253, 8, 0.678);}
    100%{font-size: 15rpx;color: rgb(253, 253, 8, 0.678);}
    
}
.cancel2{
    height: auto;
    animation-name: cancel2;
    animation-duration:0.3s;
    animation-fill-mode:forwards;
}
@keyframes cancel2{
    from{font-size: 15rpx;}
    100%{font-size: 30rpx;color: rgba(102, 102, 102);}
    
}


.cancelcol{
    
    animation-name: cancel;
    animation-duration:1s;
    animation-fill-mode:forwards;
}
